{
    "name": "shopware/production",
    "type": "project",
    "license": "MIT",
    "config": {
        "optimize-autoloader": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Shopware\\Production\\": "src/"
        }
    },

    "scripts": {
        "pre-install-cmd": [
            "test ! -r vendor/autoload.php || bin/console system:update:prepare"
        ],
        "post-install-cmd": [
            "bin/build-js.sh",
            "bin/console system:update:finish || true"
        ]
    },
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "shopware/recovery",
                "version":"master",
                "source": {
                    "url": "https://github.com/shopware/recovery.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        }
    ],

    "require": {
        "php": "~7.2",
        "ocramius/package-versions": "1.4.0",
        "shopware/core": "dev-master#6ef05579fe9a90b0121da3a39af78c363b9d38da",
        "shopware/administration": "dev-master#2a8b872622c49d826a8858098a8ea340791a89a6",
        "shopware/storefront": "dev-master#9232550f729f33a2d74415748863584e1fb66624",
        "shopware/elasticsearch": "dev-master#95976d28a064b0257269e00c9ac076c9f4229465",
        "shopware/recovery": "dev-master#90fa97aac102eb6cf367c2a1526bf346044ed4e7"
    },
    "require-dev": {
        "ext-tokenizer": "*",
        "ext-xmlwriter": "*",
        "ext-openssl": "*",
        "bheller/images-generator": "~1.0.1",
        "fzaninotto/faker": "~1.8.0",
        "johnkary/phpunit-speedtrap": "~3.1.0",
        "league/flysystem-memory": "~1.0.2",
        "mbezhanov/faker-provider-collection": "~1.2.1",
        "phpunit/phpunit": "~8.3.4",
        "smalot/pdfparser": "~0.14.0",
        "symfony/browser-kit": "~4.3",
        "symfony/dependency-injection": "~4.3",
        "symfony/phpunit-bridge": "~4.3",
        "symfony/stopwatch": "~4.3",
        "symfony/var-dumper": "~4.3",
        "symfony/web-profiler-bundle": "~4.3",
        "defuse/php-encryption": "~2.2.1",
        "symfony/dom-crawler": "~4.3.8"
    }
}
